        <script>
            function applayCoupon(Id, userId) {
                let coupon = document.getElementById(Id).value
                $.ajax({
                    type: 'POST',
                    url: '/applyCoupon',
                    data: {
                        coupon: coupon,
                        userId: userId
                    },
                    success: function (response) {
                        console.log(response)
                        if (response.total) {
                            document.getElementById('totalvalue').innerHTML = response.total
                            document.getElementById('sucessMessage').innerHTML = response.message
                        } else {
                            document.getElementById('errMessage').innerHTML = response.message
                        }
                    }
                })
            }
